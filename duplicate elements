class Solution {
    public boolean containsDuplicate(int[] nums) {
        int m=nums.length;
        Arrays.sort(nums);
        for(int i=0;i<m-1;i++)
        {
            if(nums[i]==nums[i+1])
            {
                return true;
            }
        }
        return false;
    
    }
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int nums[]=new int[n];
        for(int i=0;i<n;i++){
            nums[i]=sc.nextInt();
        }
        Solution sol=new Solution();
        boolean result=sol.containsDuplicate(nums);
        System.out.print(result);

    }
}
