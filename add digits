class Solution {
    public int addDigits(int num) {
        while(num>=10){

        int sum=0;
        while(num>0){
            int rem=num%10;
            sum=sum+rem;
            num=num/10;
        }
        num=sum;
        }
        return num;
    }
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        Solution sol=new Solution();
        int result=sol.addDigits(num);
        System.out.print(result);
    }
}
