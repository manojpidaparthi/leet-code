class Solution {
    public int missingNumber(int[] nums) {
        int m=nums.length;
        Arrays.sort(nums);
        for(int i=0;i<m;i++)
        {
            if(nums[i]!=i){
                return i;
            }
        }
        return nums.length;
    }
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int nums[]=new int[n];
        for(int i=0;i<n;i++){
            nums[i]=sc.nextInt();
        }
        Solution sol=new Solution();
        int result=sol.missingNumber(nums);
        System.out.print(result);
    }
}
